<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project pascalinterpreterinjava">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="JSPIIJ.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="com.js.interpreter.startup.Interface"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="classes"/>
            <zipfileset excludes="META-INF/*.SF" src="lib/commons-httpclient-3.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="lib/google-collect-1.0.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="lib/serp-1.14.2.jar"/>
        </jar>
    </target>
</project>
